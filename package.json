{
  "name": "@tollbrothers/blueprint",
  "version": "1.0.1",
  "description": "Toll Brothers Design System",
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tollbros/blueprint.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=16.19.0"
  },
  "scripts": {
    "build": "microbundle-crl --tsconfig tsconfig.json --compress --format modern --jsxFragment React.Fragment",
    "start": "microbundle-crl --tsconfig tsconfig.json watch --compress --format modern --jsxFragment React.Fragment",
    "prepare": "run-s build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "next lint",
    "stylelint": "stylelint '**/*.scss'"
  },
  "bugs": {
    "url": "https://github.com/tollbros/blueprint/issues"
  },
  "homepage": "https://github.com/tollbros/blueprint#readme",
  "peerDependencies": {
    "next": "^13.0.6",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.56.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-unicode-property-regex": "^7.18.6",
    "@babel/preset-react": "^7.24.7",
    "@chromatic-com/storybook": "^1.6.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/addon-onboarding": "^8.1.11",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/blocks": "^8.1.11",
    "@storybook/nextjs": "^8.1.11",
    "@storybook/react": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "autoprefixer": "^10.4.19",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "8.5.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "4.0.0",
    "glob": "^10.4.2",
    "microbundle-crl": "^0.13.10",
    "next": "^13.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.56.2",
    "semantic-release": "^19.0.5",
    "storybook": "^8.1.11",
    "stylelint": "^16.6.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^13.1.0"
  },
  "files": [
    "dist"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended",
      "plugin:storybook/recommended"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.21"
  }
